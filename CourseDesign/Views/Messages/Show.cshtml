@model IEnumerable<CourseDesign.Models.Reply>

@{
    ViewBag.Title = "Show";
}

<h2>All Replies OF This Message</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ReplyContent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReplyUserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ReplyContent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReplyUserName)
            </td>
        </tr>
    }

</table>

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

